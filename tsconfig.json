{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "$components/*": ["lib/components/*"],
      "$helpers/*": ["lib/helpers/*"],
      "$redux/*": ["lib/redux/*"],
      "$hooks/*": ["lib/hooks/*"],
      "$constants/*": ["lib/constants/*"],
      "$assets/*": ["assets/*"],
      "$services/*": ["lib/services/*"],
      "$utils/*": ["src/utils/*"],
      "$types/*": ["lib/types/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "lib/types/global.d.ts"
, "lib/helpers/PlotData.js"  ]
}
